{
    "name": "aic/website",
    "description": "Art Institute of Chicago website – artic.edu",
    "keywords": ["website", "museum", "musetech", "art institute of chicago"],
    "license": "AGPL-3.0-or-later",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/art-institute-of-chicago/data-hub-foundation.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/art-institute-of-chicago/prince-php-wrapper.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "aic/data-hub-foundation": "dev-laravel-6-support",
        "area17/twill": "2.4.*",
        "eluceo/ical": "0.14.*",
        "fideloper/proxy": "~4.2",
        "guzzlehttp/guzzle": "6.3.0",
        "intervention/httpauth": "^2.1",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^2.5",
        "league/csv": "^9.5",
        "michelf/php-smartypants": "^1.8",
        "netresearch/composer-patches-plugin": "^1.3",
        "ramsey/uuid": "^3.7",
        "rlanvin/php-rrule": "2.*",
        "salesforce-mc/fuel-sdk-php": "^1.4",
        "sentry/sentry-laravel": "^1.3.1",
        "spatie/calendar-links": "^1.0",
        "spatie/laravel-feed": "^2.4.1",
        "spatie/laravel-sitemap": "^5.4",
        "yeslogic/prince-php-wrapper": "dev-master"
    },
    "require-dev": {
        "facade/ignition": "^1.16.15",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^4.0",
        "laravel/homestead": "^6.5",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "~5.0|~6.0|~7.0|~8.0|~9.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "~3.3|~3.4|~3.5|~3.6|~3.7|~3.8|^4.0|^5.0|^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "area17/twill": {
                "2.*": [
                    {
                        "title": "WEB-2103: Get repository model of API models",
                        "url": "patches/WEB-2301---get-repository-of-API-models.diff"
                    }
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "conflict": {
        "symfony/polyfill-php80": "*"
    }
}
