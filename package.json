{
  "name": "aic",
  "version": "0.2.2",
  "private": true,
  "scripts": {
    "dev": "run-p svg-sprite vite:watch sass:watch",
    "build": "run-p svg-sprite vite:build sass:build",
    "sass:build": "sass ./frontend/scss:public/dist/styles -s compressed",
    "sass:watch": "sass ./frontend/scss:public/dist/styles -s compressed --watch",
    "presvg-sprite": "mkdirp ./public/dist/icons",
    "svg-sprite": "svg-symbol-sprite -i ./frontend/icons -o ./public/dist/icons/icons.svg -p icon-- -c false",
    "vite:build": "vite build --mode production",
    "vite:watch": "vite build --watch --mode development",
    "vite:analyze": "vite-bundle-visualizer"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "cross-env": "^7.0.3",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "svg-symbol-sprite": "^1.5.2",
    "vite": "^5.4.0",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "dependencies": {
    "@area17/a17-helpers": "^0.6.7",
    "@sentry/react": "^8.26.0",
    "@sketchfab/viewer-api": "^1.12.1",
    "closer-look": "git+ssh://git@github.com/jonw-cogapp/digital-labels-ui.git#master",
    "fitty": "^2.4.2",
    "gl-vec3": "^1.1.3",
    "lodash": "^4.17.21",
    "mirador": "^3.0.0",
    "my-museum-tour-builder": "git+https://github.com/art-institute-of-chicago/my-museum-tour.git#semver:^1.15",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "video.js": "^8.17.2",
    "videojs-event-tracking": "^1.0.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
